<UserControl xmlns:WordleHelper="clr-namespace:WordleHelper"  x:Class="WordleHelper.HintKeyBoard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <WordleHelper:HintKey Grid.Column="0"/>
        <WordleHelper:HintKey Grid.Column="1"/>
        <WordleHelper:HintKey Grid.Column="2"/>
        <WordleHelper:HintKey Grid.Column="3"/>
        <WordleHelper:HintKey Grid.Column="4"/>
    </Grid>
</UserControl>
